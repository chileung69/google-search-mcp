{
  "name": "google-search-mcp",
  "version": "1.0.0",
  "description": "Google Search MCP Server - Provides Google search functionality with intelligent result summarization for AI assistants",
  "private": false,
  "type": "module",
  "bin": {
    "google-search-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
